<% medicines.forEach(function(medicine,index){ %>
  <tr>
    <th scope="row"><%= index+1 %></th>
    <td><%= medicine.medicineName %></td>
    <td><%= medicine.medicinePrice %></td>
    <td><%= medicine.medicineCategory %></td>
    <td><%= medicine.medicineAmount %></td>
    <td>
      <a href="/medicine/delete-medicine/<%= medicine._id %>" class="delete-button btn btn-danger btn-sm mr-2">Delete</a>
      <a href="/medicine/edit-medicine/<%= medicine._id %>" class="btn btn-dark btn-sm mr-2">Edit</a>
    </td>
  </tr>
<% }) %>      